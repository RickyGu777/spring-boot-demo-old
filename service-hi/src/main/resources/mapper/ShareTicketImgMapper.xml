<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.servicehi.dao.ShareTicketImgDao">
    <resultMap id="ShareTicketUrlResult" type="com.example.servicehi.entity.ShareTicketImg">
        <result column="ID" property="id"/>
        <result column="UUID" property="uuid"/>
        <result column="QR_CODE" property="QRCode"/>
        <result column="UPLOAD_IMG_UUID" property="uploadImgUUID"/>
        <result column="IS_DEL" property="isDel"/>
        <result column="STATUS" property="status"/>
        <result column="CREATE_USER_UUID" property="createUserUUID"/>
        <result column="CREATE_DATE" property="createDate"/>
        <result column="MODI_USER_UUID" property="modiUserUUID"/>
        <result column="MODI_DATE" property="modiDate"/>
    </resultMap>

    <insert id="insert" parameterType="ShareTicketUrl">
        insert into SHARE_TICKET_IMG (UUID, QR_CODE, UPLOAD_IMG_UUID, CREATE_USER_UUID, CREATE_DATE, MODI_USER_UUID, MODI_DATE)
        values (#{uuid},#{QRCode},#{uploadImgUUID},#{createUserUUID},#{createDate},#{modiUserUUID},#{modiDate});
    </insert>

</mapper>